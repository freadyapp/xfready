  {
    "name": "Fready",
    "version": "0.0.3",
    "description": "Spend less time reading articles.",
    "permissions": [
      "storage",
      "activeTab",
        "http://localhost:3000/*"
    ],
    "background": {
      "scripts": [
        "third_party/jquery.min.js",
        "helpers.js",
        "dev.js",
        "background.js"
      ],
      "persistent": true
    },
    "browser_action": {
      "default_icon": {
        "16": "assets/logos/fready_logo_16.png",
        "32": "assets/logos/fready_logo_32.png",
        "48": "assets/logos/fready_logo_48.png",
        "64": "assets/logos/fready_logo_64.png",
        "128": "assets/logos/fready_logo_128.png",
        "256": "assets/logos/fready_logo_256.png",
        "512": "assets/logos/fready_logo_512.png"
      }
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Triggers Fready"
      }
    },
    "icons": {
      "16": "assets/logos/fready_logo_16.png",
      "32": "assets/logos/fready_logo_32.png",
      "48": "assets/logos/fready_logo_48.png",
      "64": "assets/logos/fready_logo_64.png",
      "128": "assets/logos/fready_logo_128.png",
      "256": "assets/logos/fready_logo_256.png",
      "512": "assets/logos/fready_logo_512.png"
    },
    "manifest_version": 2
  }